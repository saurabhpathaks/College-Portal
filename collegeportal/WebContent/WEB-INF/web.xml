<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>collegeportal</display-name>
  <welcome-file-list>
   
    <welcome-file>/jsp/index.jsp</welcome-file>
    
  </welcome-file-list>
  <error-page>
  <exception-type>
  
  java.lang.Exception
  </exception-type>
  <location>/errorpage/customerror.jsp</location>
  
  </error-page>
  <error-page>
  <error-code>404</error-code>
  <location>/errorpage/pagenotfound.jsp</location>
  
  </error-page>
  
  
  <context-param>
  <param-name>Tagline</param-name>
  <param-value>Spread The Joy Of Learning</param-value>
  
  
  </context-param>
  <context-param>
  <param-name>Year</param-name>
  <param-value>2000</param-value>
  
  </context-param>
</web-app>